CLASS Person
FUNC constructor:
CONTEXT LOCAL LOAD name
STORE name
CONTEXT LOCAL LOAD age
STORE age
STOP

CONST "x"
STORE name
DECLARE age
FUNC greet:
CONST "My name is "
CALL getName
OP + 1 2 3
CONST " and i'm "
OP + 3 4 5
CALL getAge
OP + 5 6 7
CONST " years old"
OP + 7 8 9
SYSCALL println 9
STOP

FUNC getName:
LOAD name
RETURN name
STOP

FUNC getAge:
LOAD age
RETURN age
STOP

CLASSEND

FUNC main:
CONST "John"
CONST 16
NEW Person 10 11
STORE john
LOAD john
CALL john#greet
STOP