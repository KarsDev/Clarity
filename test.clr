class Person {
    var name
    var age

    constructor(name, age) {
        local.name = name
        local.age = age
    }

    fn greet() {
        native.println("My name is " + local.getName() + " and i'm " + local.getAge() + " years old")
    }

    fn getName() {
        return name
    }

    fn getAge() {
        return age
    }
}

fn main() {
    var john = new Person("John", 16)

    getVoid()

    var x

    x = 5

    john.greet()

    x = 10

    native.println(x)

    native.error.except("except")
}

fn getVoid() {
    return void
}