const native class Reflections {
    constructor() {
        raise "You can not instantiate Reflection class"
    }

    /##
     # Retrieves the class of the caller of the function calling this funciton, and returns null if none is found.
     #/
    native fn getCallerClass() -> str

    /#
     # Returns whether a class is native or not.
     # Parameters:
     #
     # - name: The class name.
     #/
    native fn isClassNative(name) -> bool
}